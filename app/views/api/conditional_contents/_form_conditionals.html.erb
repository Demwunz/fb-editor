<%= f.fields_for :conditionals, child_index: conditional_index do |conditional| %>
  <div class="content-visibility" data-conditional-index="<%= conditional.index %>">
    <%= render 'form_expressions', conditional: conditional %>
  </div>
  <% unless conditional.options[:child_index] == @conditional_content.conditionals.length - 1 %>
    <div>or</div>
  <% end %>
<% end %>



